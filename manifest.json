{
  "name": "Website Blocker",
  "author": "Henry Zheng",
  "version": "1.1.0",
  "manifest_version": 3,
  "description": "Blocks websites you specify by redirecting to a blocked page",
  "permissions": ["storage", "tabs", "webNavigation"],
  "host_permissions": ["<all_urls>"],
  "background": {
    "service_worker": "background.js"
  },
  "options_page": "options.html",
  "action": {
    "default_title": "My Site Blocker"
  }
}
